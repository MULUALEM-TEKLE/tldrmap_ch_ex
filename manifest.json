{
	"name": "Gemini Markmap Extension",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content-scripts.js"],
			"run_at": "document_start"
		}
	],
	"permissions": ["activeTab", "storage", "scripting"]
}
